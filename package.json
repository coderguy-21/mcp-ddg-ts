{
  "name": "mcp-websearch-server",
  "version": "1.0.0",
  "description": "MCP server for intelligent web search with DuckDuckGo and Brave fallback, plus content fetching",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "test": "npm run build && node tests/test-provider-refactor.mjs",
    "test:providers": "npm run build && node tests/test-provider-refactor.mjs",
    "test:brave": "npm run build && node tests/test-brave.mjs",
    "gencerts": "node gencerts.js"
  },
  "keywords": [
    "mcp",
    "websearch",
    "duckduckgo",
    "bing",
    "search",
    "fetch",
    "fallback"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "latest",
    "@types/cors": "^2.8.19",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
